{
	"name": "@plavna/design",
	"repository": "https://github.com/denlukia/plavna/",
	"version": "0.0.2",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"exports": {
		"./theming/styles": {
			"default": "./src/lib/theming/styles/index.css"
		},
		"./theming/*": {
			"default": "./src/lib/theming/*.ts"
		},
		"./*": {
			"default": "./src/lib/*/index.ts"
		}
	},
	"peerDependencies": {
		"svelte": "^5.34.7"
	},
	"devDependencies": {
		"@plavna/common": "workspace:*",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.3.1",
		"publint": "0.3.12",
		"svelte-check": "^4.0.5",
		"vite": "^6.2.5"
	},
	"type": "module",
	"dependencies": {
		"bezier-easing": "^2.1.0"
	}
}
