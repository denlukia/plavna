{
	"name": "@plavna/design",
	"repository": "https://github.com/denlukia/plavna",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"exports": {
		"./theming/*": {
			"types": "./src/lib/theming/*.ts",
			"svelte": "./src/lib/theming/*.ts",
			"default": "./src/lib/theming/*.ts"
		},
		"./theming/styles": {
			"default": "./src/lib/theming/styles/index.css"
		},
		"./*": {
			"types": "./src/lib/*/index.ts",
			"svelte": "./src/lib/*/index.ts",
			"default": "./src/lib/*/index.ts"
		}
	},
	"peerDependencies": {
		"svelte": "^5.10.0"
	},
	"devDependencies": {
		"@plavna/common": "workspace:*",
		"@sveltejs/kit": "^2.7.2",
		"@sveltejs/package": "^2.3.1",
		"publint": "^0.1.16",
		"svelte-check": "^4.0.5",
		"vite": "^5.4.11"
	},
	"type": "module",
	"dependencies": {
		"bezier-easing": "^2.1.0"
	}
}
