{
	"name": "@plavna/image-uploader",
	"repository": "https://github.com/denlukia/plavna",
	"version": "0.0.39",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"main": "dist/index.js",
	"exports": {
		"./images": {
			"types": "./dist/images.d.ts",
			"default": "./dist/images.js"
		},
		"./queue": {
			"types": "./dist/queue.d.ts",
			"default": "./dist/queue.js"
		},
		"./constants": {
			"types": "./dist/constants.d.ts",
			"default": "./dist/constants.js"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"default": "./dist/types.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.30.4",
		"@sveltejs/package": "^2.3.1",
		"@types/get-pixels": "^3.3.4",
		"@types/probe-image-size": "^7.2.4",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"drizzle-orm": "^0.30.10",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.39.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "^0.1.16",
		"svelte": "^4.2.16",
		"svelte-check": "^3.7.1",
		"tslib": "^2.6.2",
		"typescript": "5.5.0-beta",
		"vite": "^4.5.3",
		"ndarray": "^1.0.19"
	},
	"type": "module",
	"dependencies": {
		"extract-colors": "^4.0.6",
		"get-pixels": "^3.3.3",
		"imagekit-javascript": "^2.0.0"
	}
}
