{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"importOrder": ["^\\$lib/(.*)$", "", "^[./]", "", "<TS_TYPES>"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
