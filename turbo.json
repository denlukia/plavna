{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"env": [
				"PUBLIC_HOST",
				"DATABASE_AUTH_TOKEN",
				"DATABASE_URL",
				"GITHUB_CLIENT_SECRET",
				"GITHUB_CLIENT_ID",
				"GITHUB_PKG_AUTH_TOKEN",
				"SCREENSHOTTER_ACCESS_TOKEN",
				"ENABLE_EXPERIMENTAL_COREPACK",
				"SENTRY_AUTH_TOKEN"
			],
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".svelte-kit/**", ".vercel/**", "dist/**"]
		},
		"dev": {
			"dependsOn": ["^build"],
			"persistent": true,
			"cache": false
		}
	}
}
